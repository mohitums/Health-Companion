<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.lenovo.healthcompanion">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
         android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".FullscreenActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name"
            android:theme="@style/FullscreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Symptom"></activity>
        <activity android:name=".Stomach"></activity>
        <activity android:name=".Stomach2"></activity>
        <activity android:name=".Cough"></activity>
        <activity android:name=".Cough2"></activity>
        <activity android:name=".Diarrhea"></activity>
        <activity android:name=".Diarrhea2"></activity>
        <activity android:name=".Headache"></activity>
        <activity android:name=".Headache2"></activity>
        <activity android:name=".ChestPain"></activity>
        <activity android:name=".Chestpain2"></activity>
        <activity android:name=".Nausea"></activity>
        <activity android:name=".Nausea2"></activity>
        <activity android:name=".Genmed"></activity>
        <activity android:name=".WaterReminder"></activity>
        <activity android:name=".Calorie"></activity>
        <activity android:name=".PillReminder"></activity>
        <activity android:name=".Food"></activity>
        <activity android:name=".Doctor"></activity>
        <activity android:name=".Doctor2"></activity>
        <receiver android:name=".TimeAlarm" />
        <receiver android:name=".AlarmReceiver"/>

        </application>

</manifest>